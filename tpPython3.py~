import os,sys

dico = {}

def parcours(repertoire):
        
        print("je suis dans " + repertoire)
        liste = os.listdir(repertoire)
        
        for fichier in liste :
                print(fichier)

chemin = sys.argv[1]
print("test")
print(chemin)
parcours(chemin)
